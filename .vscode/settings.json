{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "dart.onlyAnalyzeProjectsWithOpenFiles": true,
  "dart.projectSearchDepth": 3,
  "dart.analysisExcludedFolders": [
    "**/node_modules",
    "**/dist",
    "**/build",
    ".nx",
    "coverage",
    "node_modules",
    "tooling",
    "languages/kotlin",
    "languages/ts",
    "languages/cpp",
    "dist",
    "tests/server",
    "tests/clients/ts",
    "tests/clients/kotlin",
    "tests/clients/rust"
  ],
  "cSpell.words": [
    "Arri",
    "arrirpc",
    "autoroutes",
    "chrono",
    "citty",
    "codegen",
    "Consola",
    "degit",
    "joshmossas",
    "kotlinx",
    "ktor",
    "listhen",
    "middlewares",
    "modii",
    "modiimedia",
    "nitropack",
    "nocheck",
    "ofetch",
    "outfile",
    "pathe",
    "peekable",
    "postversion",
    "pubspec",
    "Quickstart",
    "Requirable",
    "reqwest",
    "scule",
    "serde",
    "struct",
    "structs",
    "typebox",
    "typeof",
    "uncrypto",
    "unflatten",
    "vals",
    "websockets"
  ],
  "rust-analyzer.linkedProjects": [
    "${workspaceFolder}/languages/rust/rust-client/Cargo.toml",
    "${workspaceFolder}/languages/rust/rust-codegen-reference/Cargo.toml",
    "${workspaceFolder}/tests/clients/rust/Cargo.toml"
  ],
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "[go]": {
    "editor.formatOnSave": true,
    "editor.insertSpaces": true,
    "editor.defaultFormatter": "golang.go"
  },
  "eslint.validate": ["json"],
  "eslint.useFlatConfig": true
}
