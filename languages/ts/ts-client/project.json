{
  "name": "ts-client",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "languages/ts/ts-client/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "unbuild",
        "cwd": "languages/ts/ts-client"
      }
    },
    "publish": {
      "dependsOn": ["build"],
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm publish",
        "cwd": "languages/ts/ts-client"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm eslint languages/ts/ts-client"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "inputs": ["{projectRoot}/src"],
      "options": {
        "command": "vitest run ./src --passWithNoTests --globals",
        "cwd": "languages/ts/ts-client"
      }
    },
    "integration-test": {
      "executor": "nx:run-commands",
      "inputs": ["{projectRoot}/integration-tests", "{projectRoot}/src"],
      "options": {
        "command": "vitest run ./integration-tests --passWithNoTests --globals",
        "cwd": "languages/ts/ts-client"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "languages/ts/ts-client"
      }
    }
  },
  "tags": []
}
